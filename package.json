	},
	"homepage": "https://github.com/Faelayis/RPC-Pc-Status-BetterDiscord#readme",
	"dependencies": {
		"@types/bandagedbd__bdapi": "^0.3.4",
		"npm-cli-login": "^1.0.0"
	},
	"devDependencies": {
		"version-bump-prompt": "^6.1.0",
		"@actions/core": "^1.8.2",
